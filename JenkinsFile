pipeline{
    agent any
    stages{
    
    stage("code checkout"){
        steps{
        echo 'hello'
        }
    }
    stage("code build"){
        steps{
        echo 'build'
        }
    }
    stage("Unit test"){
        steps{
        echo 'unit'
        }
    }
 }
 post{
    always{
        //Execute every time
    }
    success{
        echo 'success'
    }
 
 }
}
