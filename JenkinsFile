pipeline{
    agent any
    tools{
        maven 'Maven'
    }
    stages{
    
    stage("code checkout"){
        steps{
        echo 'hello'
        }
    }
    stage("code build"){
        steps{
        echo 'mvn clean'
        }
    }
    stage("Unit test"){
        steps{
        echo 'mvn test'
        }
    }
 }
 post{
   
    success{
        echo 'success'
    }
 
 }
}
